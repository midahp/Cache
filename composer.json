{
    "minimum-stability": "dev",
    "name": "horde/cache",
    "description": "Caching library",
    "type": "library",
    "homepage": "https://www.horde.org/libraries/Horde_Cache",
    "license": "LGPL-2.1",
    "authors": [
        {
            "name": "Chuck Hagenbuch",
            "email": "chuck@horde.org",
            "role": "lead"
        },
        {
            "name": "Michael Slusarz",
            "email": "slusarz@horde.org",
            "role": "lead"
        }
    ],
    "time": "2021-11-19",
    "repositories": [
        {
            "type": "composer",
            "url": "https://horde-satis.maintaina.com/"
        }
    ],
    "require": {
        "php": "^7.4 || ^8",
        "horde/compress_fast": "^2 || dev-FRAMEWORK_6_0",
        "horde/exception": "^3 || dev-FRAMEWORK_6_0",
        "horde/util": "^3 || dev-FRAMEWORK_6_0",
        "ext-hash": "*"
    },
    "require-dev": {
        "horde/test": "^3 || dev-FRAMEWORK_6_0",
        "horde/db": "^3 || dev-FRAMEWORK_6_0",
        "horde/hashtable": "^2 || dev-FRAMEWORK_6_0",
        "horde/log": "^3 || dev-FRAMEWORK_6_0",
        "horde/memcache": "^3 || dev-FRAMEWORK_6_0",
        "horde/mongo": "^2 || dev-FRAMEWORK_6_0"
    },
    "suggest": {
        "horde/db": "^3 || dev-FRAMEWORK_6_0",
        "horde/hashtable": "^2 || dev-FRAMEWORK_6_0",
        "horde/log": "^3 || dev-FRAMEWORK_6_0",
        "horde/memcache": "^3 || dev-FRAMEWORK_6_0",
        "horde/mongo": "^2 || dev-FRAMEWORK_6_0",
        "ext-eaccelerator": "0.9.5",
        "ext-xcache": "*",
        "ext-apcu": "*"
    },
    "autoload": {
        "psr-0": {
            "Horde_Cache": "lib/"
        },
        "psr-4": {
            "Horde\\Cache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Horde\\Cache\\Test\\": "test/"
        }
    }
}